{
    "name": "C3 file browser",
    "description": "Application to browse files stored in C3 system",
    "version": "0.2",
    "manifest_version": 2,
    "permissions": [
        "http://*/",
        "clipboardRead",
        "clipboardWrite",
        "fileSystem",
        "webview",
        "storage",
        "contextMenus"
    ],
    "app": {
        "background": {
            "scripts": ["js/background.js"]
        }
    },
    "icons": { "16": "img/terminal-16.png", "128": "img/terminal-128.png" }
}